function [a] = memory_sdp(lowers, uppers, weights, biases, c)

    W1 = weights.10;

%     cvx_begin sdp
%     cvx_solver mosek
%     
%     variable x0(784,1) 
%     variable x1(50,1)
%     variable x2(10,1)
%     variable Q0(784,784)
%     variable R0(50,784)
%     variable Q1(50,50)
%     variable R1(10,50)
%     variable Q2(10,10)
% 
%     
%     maximize( dot(c, x2) )
%     subject to
%         diag(Q1) == diag(R_0 * W0' + x1 * b1');
%         diag(Q2) == diag(R_1 * W1' + x2 * b2');
%         
%         x1 >= max(0, W0 * x0 + b0);
%         x2 >= max(0, W1 * x1 + b1);
%         
%         x0_lower <= x0;
%         x0 <= x0_upper;
%         
%         x1_lower <= x1;
%         x1 <= x1_upper;
%         
%         [Q0 R0' x0; R0 Q1 x1; x0' x1' 1] >= 0;
%         [Q1 R1' x1; R1 Q2 x2; x1' x2' 1] >= 0;
%         
%     cvx_end

    a = 1;

end